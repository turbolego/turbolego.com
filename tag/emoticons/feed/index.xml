					<comments>https://turbolego.github.io/turbolego.com//chat-codes-you-can-remember/#comments</comments>
		
		<dc:creator><![CDATA[turbolego]]></dc:creator>
		<pubDate>Fri, 12 Oct 2012 23:07:19 +0000</pubDate>
				<category><![CDATA[Chat Codes]]></category>
		<category><![CDATA[chat]]></category>
		<category><![CDATA[chrome]]></category>
		<category><![CDATA[code]]></category>
		<category><![CDATA[codes]]></category>
		<category><![CDATA[emoticons]]></category>
		<category><![CDATA[facebook]]></category>
		<category><![CDATA[firefox]]></category>
		<category><![CDATA[google]]></category>
		<category><![CDATA[google chrome]]></category>
		<category><![CDATA[likes]]></category>
		<category><![CDATA[nickname]]></category>
